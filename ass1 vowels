#include<stdio.h>
#include<string.h>
main()
{
char ch[10];
int n,i;
char c;
int count=0;
printf("enter name");
gets(ch);
n=strlen(ch);
for(i=0;i<n;i++)
{
	c=ch[i];
	switch(c)
	{
		case 'A':
        case 'a':
		case 'E':		
		case 'e':
		case 'i': 
		case 'I':
		case 'o':
		case 'O':
		case 'u':
		case 'U':
			case 'a'&'z':
			case 'A'&'Z':	
		count++;
		f++;
		break;
		default:("%c",c)	
	}
}
printf("total no.of vowels%d",count);
printf("total no.of consonants%d",f);
}
